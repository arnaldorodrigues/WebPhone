{
  "name": "browser_phone",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "concurrently \"next dev --turbopack --experimental-https\" \"node --loader ts-node/esm sms-server.js\"",
    "build": "NODE_OPTIONS='--max-old-space-size=4096' next build",
    "start": "NODE_ENV=production next start",
    "lint": "next lint",
    "seed:admin": "node scripts/seed-admin.js",
    "postinstall": "patch-package",
    "socket": "node --loader ts-node/esm scripts/start-socket-server.ts",
    "dev:socket": "node --loader ts-node/esm scripts/start-socket-server.ts",
    "start:socket": "NODE_ENV=production node --loader ts-node/esm scripts/start-socket-server.ts",
    "start:next": "next start",
    "dev:next": "next dev --turbopack --experimental-https",
    "start-sms": "node --loader ts-node/esm sms-server.js",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:watch:ui": "vitest --ui"
  },
  "dependencies": {
    "@headlessui/react": "^2.2.4",
    "@heroicons/react": "^2.2.0",
    "@reduxjs/toolkit": "^2.8.2",
    "@signalwire/compatibility-api": "^3.0.5",
    "@types/bcryptjs": "^2.4.6",
    "@types/ws": "^8.5.10",
    "bcryptjs": "^3.0.2",
    "body-parser": "^2.2.0",
    "concurrently": "^8.2.2",
    "date-fns": "^4.1.0",
    "dotenv": "^16.6.0",
    "express": "^5.1.0",
    "js-cookie": "^3.0.5",
    "jsonwebtoken": "^9.0.2",
    "jwt-decode": "^4.0.0",
    "modesl": "^0.0.4",
    "mongoose": "^8.15.1",
    "next": "15.3.3",
    "next-auth": "^5.0.0-beta.28",
    "pg": "^8.16.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-icons": "^5.5.0",
    "react-redux": "^9.2.0",
    "sip.js": "^0.21.2",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ws": "^8.18.2"
  },
  "devDependencies": {
    "@apidaze/node": "github:apidaze/sdk-node",
    "@eslint/js": "^9.32.0",
    "@tailwindcss/postcss": "^4.1.10",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/jwt-decode": "^2.2.1",
    "@types/modesl": "^1.1.6",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "eslint-plugin-react": "^7.37.5",
    "globals": "^16.3.0",
    "jiti": "^2.5.1",
    "jsdom": "^24.0.0",
    "patch-package": "^8.0.0",
    "postinstall-postinstall": "^2.1.0",
    "tailwindcss": "^4",
    "ts-node": "^10.9.2",
    "typescript": "^5",
    "typescript-eslint": "^8.38.0",
    "vitest": "^1.3.1"
  }
}
